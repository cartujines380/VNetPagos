@model ICollection<VisaNet.Domain.EntitiesDtos.HighwayEmailErrorDto>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Errores de procesamiento del archivo de Carretera</h2>
<div class="row">
    <div class="col-md-10">
        <ul class="list-group">
            @foreach (var error in Model)
            {
                <li class="list-group-item">@error.Data</li>
            }
        </ul>
    </div>
    <div class="col-md-5">
        <a href="@Url.Action("Index")" class="btn btn-default btn-cancel">Volver </a>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {

    });
</script>
