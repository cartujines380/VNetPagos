@using VisaNet.Common.Resource.Presentation
@model VisaNet.Common.Logging.Entities.LogDto
<div id="lbDetails" class="modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <div class="row">
                    <div class="col-xs-5">
                        <a class="brandVisaNet fLeft" href="/"> </a>
                    </div>
                    <div class="col-xs-6"><a class="brandVisa fRight"></a></div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="modal-body">
                <h4 class="modal-title">@PresentationCallCenterStrings.AuditLog</h4>
                <div class="form-group">
                    @Html.LabelFor(model => model.UserName)
                    @Html.DisplayFor(model => model.UserName)
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.LogType)
                    @Html.DisplayFor(model => model.LogType)
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.DateTime)
                    @Html.DisplayFor(model => model.DateTime)
                </div>
                <div class="form-group">
                    @Html.LabelFor(model => model.Message)
                    @Html.DisplayFor(model => model.Message)
                </div>
                </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-xs-6 p5l">
                        <button type="button" class="btn btn-default btn-siguiente w100" data-dismiss="modal">@PresentationAdminStrings.Back <i class="icon-chevron-sign-right"></i></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
